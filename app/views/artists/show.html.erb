# artists shows the song count for each artist
# artists lists all of the artist's songs
# artists show page links to the artist's songs by title

<h1><%= @artist.name %></h1>
<ul>
  <% @artist.songs.each do |song| %>
    <li>
      <%= link_to song.title, song_path(song) %>
    </li>
  <% end %>
</ul>